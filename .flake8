[flake8]
count = True
ignore = W503,E226,B,C,D,E,SIM105
max-line-length = 120
exclude =
    stubs/*,
    */*raining*/*,
    *dgi.json.py,
    */ivtc*/*.py,
    */*_hidden/*,
    */*sakuga*/*,
    */*TEMP_ARCHIVING/*,
    */*kaguya-sama-ova-encoding*/*,
    */*project_module*/
show-source = True
statistics = True
